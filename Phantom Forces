--Properties:
 
rivertestgui.Name = "rivertestgui"
rivertestgui.Parent = game.CoreGui
rivertestgui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
 
mainframe.Name = "mainframe"
mainframe.Parent = rivertestgui
mainframe.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
mainframe.Position = UDim2.new(0.327915877, 0, 0.114331722, 0)
mainframe.Size = UDim2.new(0, 509, 0, 241)
mainframe.Style = Enum.FrameStyle.ChatRed
 
Adminbutton.Name = "Adminbutton"
Adminbutton.Parent = mainframe
Adminbutton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
